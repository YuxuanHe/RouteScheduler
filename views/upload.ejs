<% include header %>
<link rel="stylesheet" href="styles/upload-page.css">
<div class="container" ng-controller="uploadController" id="uploadPage">
	<div class="page-header">
		<h1 >Upload Today's Appointments</h1>
	</div>
	<form class="form-inline" id="uploadForm">			
		<input type="file" class="file-input " id = "fileN" file-model="uploadFile">
		<label  class = "first-radio">
			<input type="radio" ng-model= "fileType" value="sales" checked>Sales Appointments	
		</label>
		<label>
			<input type="radio"  ng-model="fileType" value="technician">Technician Appointments
		</label>
		<br>
		<button type="submit" class="btn upload-button" ng-click = "csvJSON()">Upload</button>
	</form>		
</div>
<% include footer %>